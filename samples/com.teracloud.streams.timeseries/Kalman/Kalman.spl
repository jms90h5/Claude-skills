// Declare a use directive for the DWT primitive operator.

 
composite KalmanMain {
	type
	Timeseries = tuple < float64 index, float64 Observe >;
	
	graph
	stream<rstring stamptime, float64 price, float64 svalue, float64 s1value, float64 s2value,  float64 volume,  float64 s4value> Src= FileSource()
	{
		param
              file:"stock.csv";
		format: csv;
	} 
	
	stream<float64 price, float64 predictedObservation> KalmanStream=Kalman(Src)
	{
		param
		inputTimeSeries: price;
		controlDimension: 5u;
		parameterFile:"etc/Parameters.dat";
	       output
       	 KalmanStream: 
              predictedObservation=predictedTimeSeries();

	}
		
	() as Snk=FileSink(KalmanStream)
	{
		param
		file: "KalmanOut.dat";
		format: csv;
	}
	}
